import { Route, Navigate } from "react-router-dom";

// Composant de vérification d'authentification
const PrivateRoute = ({ element: Element, ...rest }) => {
	// Vérifiez ici si l'utilisateur est authentifié en tant qu'administrateur
	const isAuthenticated = true; // Mettez votre logique d'authentification ici

	return <Route {...rest} element={isAuthenticated ? <Element /> : <Navigate to="/login" replace />} />;
};

export default PrivateRoute;
